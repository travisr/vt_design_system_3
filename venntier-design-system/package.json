{
  "name": "venntier-design-system",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run build:lib && ng serve demo",
    "build": "ng build",
    "build:lib": "ng build @venntier/design-system --configuration=production",
    "build:lib:watch": "ng build @venntier/design-system --watch",
    "build:demo": "npm run build:lib && ng build demo --configuration=production",
    "serve:demo": "ng serve demo",
    "dev": "npm run build:lib && concurrently \"npm run build:lib:watch\" \"npm run serve:demo\"",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "pack:lib": "cd dist/design-system && npm pack"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.2.14",
    "@angular/cdk": "^19.2.19",
    "@angular/common": "^19.2.0",
    "@angular/compiler": "^19.2.0",
    "@angular/core": "^19.2.0",
    "@angular/forms": "^19.2.0",
    "@angular/material": "^19.2.19",
    "@angular/platform-browser": "^19.2.0",
    "@angular/platform-browser-dynamic": "^19.2.0",
    "@angular/router": "^19.2.0",
    "@fontsource/inter": "^5.2.6",
    "playwright": "^1.54.2",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.15",
    "@angular/cli": "^19.2.3",
    "@angular/compiler-cli": "^19.2.0",
    "@types/jasmine": "~5.1.0",
    "concurrently": "^9.2.0",
    "jasmine-core": "~5.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^19.2.0",
    "typescript": "~5.7.2"
  }
}
